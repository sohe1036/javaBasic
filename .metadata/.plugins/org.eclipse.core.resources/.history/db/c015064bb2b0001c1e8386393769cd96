package sec1;

public class Bread {
	
	private String name;
	private String matrial;
	private int gram;
	private int price;
	private int cnt;
	
	Bread(){ }

	public Bread(String name, String matrial, int gram, int price, int cnt) {
		super();
		this.name = name;
		this.matrial = matrial;
		this.gram = gram;
		this.price = price;
		this.cnt = cnt;
	}
	
	}
	
}
